{
    "name": "ohdearapp/health-check-results",
    "description": "Create application health check results for Oh Dear",
    "keywords": [
        "ohdear",
        "monitoring",
        "application",
        "health",
        "health-check-results"
    ],
    "homepage": "https://github.com/ohdearapp/health-check-results",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/pest-plugin-snapshots": "^1.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "OhDear\\HealthCheckResults\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhDear\\HealthCheckResults\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
